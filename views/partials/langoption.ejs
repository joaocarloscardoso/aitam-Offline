<% catalog.forEach(function(item) { %>
    <%  if (item.code == sessionlang) { %>
        <input type="radio" name="languageSel" value="<%= item.code %>" onclick="handleClick(this);" checked="checked"/> <%= item.name %>
    <% } else { %>
        <input type="radio" name="languageSel" value="<%= item.code %>" onclick="handleClick(this);"/> <%= item.name %>
    <% } %>
    <br/>

<% }) %>